<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.crm.workbench.dao.TranHistoryDao">
	<insert id="add">
        INSERT INTo t_tran_history VALUES(#{id},#{stage},#{money},#{expectedDate},#{createBy},#{createTime},#{tranId})
    </insert>
    <select id="getTranHistoryByTranId" resultType="tranHistory">
        SELECT * FROM t_tran_history WHERE tranId = #{tranId}
    </select>
  	
</mapper>

































