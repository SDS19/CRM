<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.crm.workbench.dao.ContactsRemarkDao">
    <insert id="addContact">
        INSERT INTO t_customer_remark(id,noteContent,createBy,createTime,editFlag,contactsId)
        VALUES(#{id},#{noteContent},#{createBy},#{createTime},#{editFlag},#{contactsId},)
    </insert>
  	
</mapper>

































